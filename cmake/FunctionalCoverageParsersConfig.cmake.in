# FunctionalCoverageParsersConfig.cmake.in
# CMake package configuration file

@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Check if targets are already imported
if(NOT TARGET FunctionalCoverageParsers::FunctionalCoverageParsers)
    include("${CMAKE_CURRENT_LIST_DIR}/FunctionalCoverageParsersTargets.cmake")
endif()

# Create alias targets for easier usage
if(TARGET FunctionalCoverageParsers AND NOT TARGET FunctionalCoverageParsers::FunctionalCoverageParsers)
    add_library(FunctionalCoverageParsers::FunctionalCoverageParsers ALIAS FunctionalCoverageParsers)
endif()

if(TARGET FunctionalCoverageParsers_static AND NOT TARGET FunctionalCoverageParsers::FunctionalCoverageParsers_static)
    add_library(FunctionalCoverageParsers::FunctionalCoverageParsers_static ALIAS FunctionalCoverageParsers_static)
endif()

# Set variables for backward compatibility
set(FunctionalCoverageParsers_FOUND TRUE)
set(FunctionalCoverageParsers_VERSION @PROJECT_VERSION@)

check_required_components(FunctionalCoverageParsers)